// Colors for Test Material
// These Color material will be used by cone, tritower and sphere

material Crowd/Black
{
	technique
	{
		pass
		{
			shading gouraud

			ambient 0.50000 0.50000 0.50000 1.00000
			diffuse 0.00 0.00 0.00000 0.0000
			specular 0.50000 0.50000 0.50000 1.00000 3.00000
			emissive 0.00000 0.00000 0.00000 1.00000

		}
	}
}

