material solidwhiteTrail
{
	receive_shadows off
	technique
	{
		pass
		{
			lighting off	
			scene_blend alpha_blend
			depth_check off
			
			// Texture unit 0
			texture_unit
			{
				texture trail.png
				tex_coord_set 0
				colour_op modulate
				tex_address_mode clamp
			}
		}
	}
}

