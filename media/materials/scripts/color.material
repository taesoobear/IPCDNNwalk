material use_vertexcolor
{
    technique
    {
        pass
        {
		shading gouraud
		ambient vertexcolour
		diffuse vertexcolour
		specular 0.50000 0.50000 0.50000 1.00000 0.00000
		emissive 0.00000 0.00000 0.00000 1.00000
        }
    }
} 

material use_vertexcolor_transparent
{
	receive_shadows off
	technique
	{
		pass
		{
			shading gouraud
			ambient vertexcolour
			diffuse vertexcolour
			specular 0.50000 0.50000 0.50000 1.00000 0.00000
			emissive 0.00000 0.00000 0.00000 1.00000
			scene_blend alpha_blend
			depth_write off
		}
	}
}


